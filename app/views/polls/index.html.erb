<h1>List of polls</h1>

<ul>
<% @polls.each do |poll| %>
	<li><%= link_to poll.question, poll %>
		<span class='actions'>
			<%= link_to "Edit", edit_poll_path(poll) %>
			<%= link_to "Delete", poll, confirm: "Are you sure?", method: :delete %>
		</span></li>
<% end %>
</ul>

<%= link_to 'New Poll', new_poll_path %>